    "peacock.color": "#61dafb",
    "actionButtons": {
        "reloadButton": null,
        "loadNpmCommands": false,
        "commands": [
            {
                "name": "StartNetbootDemoMinimalRpi4",
                "singleInstance": true,
                "color": "#00008b",
                "command": "cd .vscode/yo; source func.sh; start_netboot_raspberrypi4",
            },
            {
                "name": "sshfsMount",
                "singleInstance": true,
                "color": "#007fff",
                "command": "mkdir /tmp/tmp_etc; sshfs user@192.168.0.1:/etc /tmp/tmp_etc",
            },
            {
                "name": "💻 🖲 📶 ⌨ 💾 📕 📤 📥 ✅ ❌ ❎ ☐ ☑ 🗳 🔧 ⚙",
                "singleInstance": true,
                "color": "#007fff",
                "command": "echo symbols_graph_utf_8",
            },
            {
                "name": "Build ◀",
                "singleInstance": true,
                "color": "#000000",
                "command": "cp -f .vscode/settings.json.build .vscode/settings.json; exit",
            },
            {
                "name": "LOAD",
                "singleInstance": true,
                "color": "#000000",
                "command": "bash",
            },
            {
                "name": "▶ Install",
                "singleInstance": true,
                "color": "#000000",
                "command": "cp -f .vscode/settings.json.install .vscode/settings.json; exit",
            }

        ]
    }
}
